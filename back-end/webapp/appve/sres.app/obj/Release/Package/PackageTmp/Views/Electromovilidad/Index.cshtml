@using sres.app.Models
@using sres.be
@using sres.ut
@{
    ViewBag.Titulo = "Menú principal";
    ViewBag.SRESDescription = false;
    UsuarioBE usuario = Session["user"] == null ? null : (UsuarioBE)Session["user"];
}

@section scripts_footer{
    <script src="@Url.Content("~/Assets/js/plugins/alert.js")"></script>
    @if (usuario.ID_ROL == (int)EnumsCustom.Roles.ADMINISTRADOR)
    {
        <script src="@Url.Content("~/Assets/js/pages/electromovilidad_menu_administrador.js")"></script>
    }
    else
    {
        <script src="@Url.Content("~/Assets/js/pages/electromovilidad_menu_usuario.js")"></script>
    }    
}

@if (usuario.ID_ROL == (int)EnumsCustom.Roles.ADMINISTRADOR)
{
    @Html.Partial("IndexAdministrador")
}
else
{
    @Html.Partial("IndexUsuario")
}

